
<!-- Image Banner -->
<div class="banner">
	<img class="banner-image" alt="banner image" src="../assets/img/handshake.jpg">
</div>




<!-- List of Journal Entries -->
<div class="container">
	<div class="row">
		<div class="col-md-8 col-md-offset-2">
			<div class="entry-container">
				<h1>Entries</h1>
				<table class="table table-striped">
					<thead>
						<tr>
							<th>Title</th>
							<th>Date</th>
							<th>Private</th>
							<th>Delete Entry</th>
						</tr>
					</thead>
					<tbody>
						<tr id="form-row">
							<form id="user-form">
								<td colspan="4">
									<input placeholder="Enter a first name" id="user-last-name" type="text" />
								</td>
								<td colspan="4">
									<input placeholder="Enter a last name" id="user-first-name" type="text" />
								</td>
								<td colspan="4">
									<input placeholder="Enter a email" id="user-email" type="text" />
								</td>
								<td colspan="4">
									<input placeholder="Enter a password" id="user-password" type="text" />
								</td>
								<td>
									<button type="submit" class="btn btn-success">Create User</button>
								</td>
							</form>
						</tr>
					</tbody>
				</table>
			</div>
		</div>
	</div>
</div>
<div>
</div>

